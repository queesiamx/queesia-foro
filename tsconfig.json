{
  "compilerOptions": {
   "noEmit": true,
    "isolatedModules": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@router/*": ["src/router/*"],
      "@components/*": ["src/components/*"]
    },
    "types": ["vite/client"]
  }
}
